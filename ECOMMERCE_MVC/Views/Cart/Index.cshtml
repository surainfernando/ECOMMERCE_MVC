@model List<ECOMMERCE_MVC.Models.Item>@{ ViewData["Title"] = "Index";}@*Shopping Cart Index*@<body>    <h1>Shopping Cart</h1>    <div class="container p-3">        <div class="row pt-4">            <div class="col-6">            </div>                   </div>        <br /><br />        @if (Model.Count() > 0)        {<table class="table table-bordered table-striped" style="width:100%">    <thead>        <tr>            <th>Image</th>            <th>                Name            </th>                       <th>Price</th>            <th></th>        </tr>    </thead>    <tbody>        @foreach (var obj in Model)        {        <tr>            <td><img src="@obj.ImageLink" width="100px"></td>            <td>@obj.Name</td>            <td>@obj.Price</td>            <td class="text-center">                <div class="w-75 btn-group" role="group">                                        @*<a asp-controller="Item1" asp-route-Id="@obj.ItemId" asp-action="EditItem" class="btn btn-danger mx-2">                    <i class="far fa-trash-alt">Delete</i>                </a>*@                    <a onclick="ConfirmDelete(@obj.ItemId)" href="#" class="btn btn-primary mx-2">                        <i class="fas fa-edit">Remove</i>                    </a>                </div>            </td>        </tr>}    </tbody></table> }            else            {<p> No category exists.</p>}    </div></body>@section Scripts{    <script type="text/javascript">		function ConfirmDelete(id) {			var r = confirm("Are you sure that you want to delete this item?");			if (r == true) {				window.location.href = '@Url.Action("DeleteItem", "Item1")/'+id;			} else {			}		}		function show() {			var x = document.getElementById("id1").value;			var r = confirm("Are you sure that you want to delete this item?"+x);		}		function storeData() {			var x = document.getElementById("id1").value;			var r = confirm("Are you sure that you want to delete this item?" + x);		}    </script>    @{                        <partial name="_ValidationScriptsPartial" /> }}